@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model List<DemoApplication.Database.Models.NavbarM>

@foreach (var navItem in Model)
{
    <div class="footer-title">
        <h3>@navItem.Title</h3>
    </div>
    <ul class="footer-list normal-list">
        @foreach (var subNav in navItem.SubNavbars)
        {
        <li><a href="#">@subNav.Title</a></li>  
        }
    
    </ul>
}
